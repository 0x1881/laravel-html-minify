{
    "name": "0x1881/laravel-html-minify",
    "description": "laravel minify html output with regex",
    "keywords": [
        "0x1881",
        "laravel",
        "laravel-html-minify"
    ],
    "homepage": "https://github.com/0X1881/laravel-html-minify",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mehmet Can",
            "email": "1881x0@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "C4N\\LaravelHtmlMinify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "C4N\\LaravelHtmlMinify\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "C4N\\LaravelHtmlMinify\\LaravelHtmlMinifyServiceProvider"
            ],
            "aliases": {
                "LaravelHtmlMinify": "C4N\\LaravelHtmlMinify\\LaravelHtmlMinifyFacade"
            }
        }
    }
}
